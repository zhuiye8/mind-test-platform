# 开发环境变量
VITE_APP_TITLE=心理测试平台（开发）
VITE_LOG_LEVEL=debug

# 针对开发后端服务的 API 地址
VITE_API_BASE_URL=/api

# Vite 开发服务器监听
VITE_DEV_SERVER_HOST=0.0.0.0
VITE_DEV_SERVER_PORT=3000
VITE_DEV_STRICT_PORT=true

# 代理至开发后端
VITE_DEV_ENABLE_PROXY=true
VITE_API_PROXY_TARGET=http://localhost:3001
VITE_API_PROXY_SECURE=false
VITE_API_PROXY_CHANGE_ORIGIN=true

# 默认关闭 HTTPS；如需启用请设置为 true 并提供证书路径
VITE_DEV_USE_HTTPS=true
VITE_DEV_CERT_PATH=./certs/192.168.0.112.pem
VITE_DEV_KEY_PATH=./certs/192.168.0.112-key.pem
